<!DOCTYPE html>

<title>Rapport index</title>

{% block body %}
    <h1>Rapport index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Periode</th>
                <th>Realisations</th>
                <th>Difficultes</th>
                <th>Propositions_innovation</th>
                <th>Auto_evaluation</th>
                <th>Evaluation_responsable</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for rapport in rapports %}
            <tr>
                <td>{{ rapport.id }}</td>
                <td>{{ rapport.periode ? rapport.periode|date('Y-m-d') : '' }}</td>
                <td>{{ rapport.realisations }}</td>
                <td>{{ rapport.difficultes }}</td>
                <td>{{ rapport.propositionsInnovation }}</td>
                <td>{{ rapport.autoEvaluation }}</td>
                <td>{{ rapport.evaluationResponsable }}</td>
                <td>
                    <a href="{{ path('app_rapport_show', {'id': rapport.id}) }}">show</a>
                    <a href="{{ path('app_rapport_edit', {'id': rapport.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="8">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_rapport_new') }}">Create new</a>
{% endblock %}
