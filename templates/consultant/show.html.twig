<!DOCTYPE html>

<title>Consultant</title>

{% block body %}
    <h1>Consultant</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ consultant.id }}</td>
            </tr>
            <tr>
                <th>Competences</th>
                <td>{{ consultant.competences }}</td>
            </tr>
            <tr>
                <th>Disponibilite</th>
                <td>{{ consultant.disponibilite ? consultant.disponibilite|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Cv</th>
                <td>{{ consultant.cv }}</td>
            </tr>
            <tr>
                <th>Description_profil</th>
                <td>{{ consultant.descriptionProfil }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_consultant_index') }}">back to list</a>

    <a href="{{ path('app_consultant_edit', {'id': consultant.id}) }}">edit</a>

    {{ include('consultant/_delete_form.html.twig') }}
{% endblock %}
